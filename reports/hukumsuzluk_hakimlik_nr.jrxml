	<queryString>
		<![CDATA[select
giden.ck_08 as gideniceriksaglayici_id,
(select code from core_keyvalue c where c.entity_type_id =  38 and status = '1'  and c.Id = giden.ck_02) as gidenhukukdosyano,
giden.code  as gidenevrakno,
(select value from core_keyvalue where ENTITY_TYPE_ID = 20 and status = '1' and Id=giden.ck_09) as kapatilanmahkeme,
giden.date_01 as gidenevraktarih,
giden.date_04 as gidenemailtarih,
(select code from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 ) as gelenevrakdosyano,
(select date_01 from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 ) as geldigiyerevraktarih,
(select date_02 from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 ) as gelenevrakislemtarihi,
(select value_01 from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 ) as geldigiyerevraksayi,
(select value from core_keyvalue m where m.entity_type_id = 20  and Id = (select ck_02 from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 )) as geldigiyer,
(select date_03 from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 ) as karartarih,
(select shortvalue from core_keyvalue c where c.ENTITY_TYPE_ID = 5 and c.Id = giden.ck_01 ) as degisilisno,
(select value from core_keyvalue m where m.entity_type_id = 20  and Id = giden.ck_05 ) as nobetcimahkeme,
(select value from core_keyvalue c where c.ENTITY_TYPE_ID = 20 and c.Id = giden.ck_08 ) as gideniceriksaglayici,
(select VALUE from core_keyvalue where ENTITY_TYPE_ID = 59 and status = '1' and code = 'Avukat_01') as avukat01,
(select VALUE from core_keyvalue where ENTITY_TYPE_ID = 59 and status = '1' and code = 'Avukat_02') as avukat02,
(select VALUE from core_keyvalue where ENTITY_TYPE_ID = 59 and status = '1' and code = 'Genel Sekreter Yrd. (Hukuk)') as hukukgyrd
FROM core_keyvalue giden
WHERE
giden.ENTITY_TYPE_ID = 27 and giden.status = '1'
and giden.ID = $P!{parameter_id}]]>
	</queryString>
